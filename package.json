{
  "name": "midi-generator",
  "version": "1.0.0",
  "description": "A node module that generates random midi files",
  "main": "app.js",
  "type": "module",
  "scripts": {
    "start": "node app.js"
  },
  "author": "Andrew Milam",
  "license": "ISC",
  "dependencies": {
    "midi-writer-js": "^2.1.4",
    "pkg": "^5.8.1",
    "tonal": "^4.14.0",
    "yargs": "^17.7.1"
  },
  "pkg": {
    "scripts": "app.js",
    "targets": [
      "node16-macos-x64",
      "node16-linux-x64",
      "node16-linux-arm64",
      "node16-alpine-x64",
      "node16-alpine-arm64",
      "node16-win-x64"
    ],
    "outputPath": "./"
  }
}
